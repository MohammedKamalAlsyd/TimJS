{
  "manifest_version": 3,
  "name": "TimJS",
  "version": "1.0.0",
  "description": "TrackJS is an AI-powered browser tool that helps you understand and improve your online behavior.",
  "icons": {
    "16": "/icons/clock.png",
    "48": "/icons/clock.png",
    "128": "/icons/clock.png"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  }  

}